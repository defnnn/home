{
  "workbench.colorTheme": "Default Dark+",
  "workbench.startupEditor": "none",
  "git.confirmSync": false,
  "editor.formatOnSave": true,
  "remote.SSH.localServerDownload": "off",
  "remote.SSH.enableRemoteCommand": true,
  "remote.SSH.useLocalServer": true,
  "remote.containers.cacheVolume": true,
  "remote.SSH.lockfilesInTmp": true,
  "remote.containers.dockerComposePath": "docker compose",
  "python.formatting.provider": "black",
  "python.analysis.typeCheckingMode": "basic",
  "python.poetryPath": "/home/ubuntu/.local/bin/poetry",
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true,
      "source.organizeImports.python": true
    }
  },
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.localEchoEnabled": "off",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.shellArgs.linux": ["-c", "export MEH=1; exec bash -l"],
  "workbench.editor.untitled.hint": "hidden"
}
